| This is a modified version of the GCD program presented in chapter 2. It has been modified to 
|  hardcode in m = 100,000,000 & n = 1.
   push 1000
   pop m                
   push 1
   pop n                  
L1 label
   push m
   push n
   sub
   tstne
   jf L2
   push m
   push n
   sub
   tstlt
   jf L3
   push n
   push m
   sub
   pop n
   j L4
L3 label
   push m
   push n
   sub
   pop m
L4 label
   j L1
L2 label
   put m
   halt